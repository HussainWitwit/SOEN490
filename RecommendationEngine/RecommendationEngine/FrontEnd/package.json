      "@fortawesome/free-solid-svg-icons": "^5.15.1",
      "@fortawesome/react-fontawesome": "^0.1.12",
      "@material-ui/core": "^4.11.0",
      "@material-ui/icons": "^4.9.1",
      "@material-ui/lab": "^4.0.0-alpha.56",
      "@material-ui/pickers": "^3.2.10",
      "boostrap": "^2.0.0",
      "bootstrap": "^4.1.3",
      "classnames": "^2.2.6",
      "date-fns": "^2.16.1",
      "jquery": "^3.5.1",
      "material-design-icons": "^3.0.1",
      "merge": "^1.2.1",
      "oidc-client": "^1.9.0",
      "react": "^16.0.0",
      "react-bootstrap": "^1.0.0-beta.14",
      "react-dates": "^21.8.0",
      "react-datetime": "^3.0.4",
      "react-dom": "^16.0.0",
      "react-hot-loader": "^4.13.0",
      "react-icons": "^3.11.0",
      "react-notification-timeline": "0.0.8",
      "react-notifications-menu": "^1.0.1",
      "react-pro-sidebar": "^0.4.4",
      "react-redux": "^7.2.2",
      "react-router-bootstrap": "^0.25.0",
      "react-router-dom": "^5.1.2",
      "react-scripts": "^4.0.0",
      "react-spring": "^8.0.27",
      "reactstrap": "^8.4.1",
      "redux": "^4.0.5",
      "redux-actions": "^2.6.5",
      "redux-batched-actions": "^0.5.0",
      "redux-thunk": "^2.3.0",
      "rimraf": "^2.6.2",
      "sass-loader": "^10.0.5",
      "spring2": "npm:react-spring@^5.7.2",
      "styled-components": "^4.4.1"
    },
    "jest": {
      "collectCoverage": true,
      "collectCoverageFrom": [
        "**/src/**",
        "!**/src/index.js",
        "!**/src/registerServiceWorker.js",
        "!**/src/tests/__snapshots__/**",
        "!**/src/tests/__snapshots__/**",
        "!**/src/api/GET/TableEndpoints.js",
        "!**/src/api/GET/AssetEndpoints.js",
        "!**/src/entities/Asset.js"
      ],
      "moduleNameMapper": {
        "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetTransformer.js",
        "\\.(css|less)$": "<rootDir>/assetTransformer.js"
      }
    },
    "prettier": {
      "singleQuote": true,
      "bracketSpacing": true,
      "useTabs": false
    },
    "husky": {
      "hooks": {
        "pre-commit": "npm run lint"
      }
    },
    "devDependencies": {
      "@babel/plugin-transform-runtime": "^7.12.1",
      "ajv": "^6.9.1",
      "babel-plugin-transform-imports": "^2.0.0",
      "cross-env": "^5.2.0",
      "cypress": "^5.5.0",
      "enzyme": "^3.11.0",
      "enzyme-adapter-react-16": "^1.15.5",
      "eslint-config-react-app": "^5.2.0",
      "eslint-plugin-flowtype": "^4.7.0",
      "eslint-plugin-import": "^2.20.1",
      "eslint-plugin-jsx-a11y": "^6.2.3",
      "eslint-plugin-react": "^7.18.3",
      "husky": "^4.3.0",
      "nan": "^2.14.1",
      "prettier": "^2.1.2",
      "start-server-and-test": "^1.11.5",
      "typescript": "^3.7.5"
    },
    "eslintConfig": {
      "extends": "react-app"
    },
    "scripts": {
      "start": "rimraf ./build && react-scripts start",
      "build": "react-scripts build",
      "test": "jest -u",
      "test:cypress": "cypress open",
      "cypress:ci": "cypress run --config video=false",
      "test:ci": "start-server-and-test start http://localhost:3000 cypress:ci",
      "eject": "react-scripts eject",
      "lint": "eslint ./src/",
      "lint:fix": "eslint --fix .",
      "format": "prettier --write \"**/*.+(js|jsx|css|md)\""
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    }
  }