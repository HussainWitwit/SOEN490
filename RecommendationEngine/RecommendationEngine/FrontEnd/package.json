{
  "name": "recommendationengine",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@material-ui/core": "^5.0.0-alpha.27",
    "@material-ui/data-grid": "^4.0.0-alpha.19",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^4.0.0-alpha.12",
    "@material/circular-progress": "^10.0.0",
    "@microsoft/signalr": "^5.0.3",
    "antd": "^4.12.3",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "isomorphic-fetch": "^3.0.0",
    "react": "^16.0.0",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.14.0",
    "react-draggable": "^4.4.3",
    "react-hot-loader": "^4.13.0",
    "react-icons": "^3.11.0",
    "react-modern-calendar-datepicker": "^3.1.6",
    "react-notifications-menu": "^1.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.0",
    "react-spring": "^8.0.27",
    "react-tabs": "^3.1.1",
    "react-toastify": "^7.0.3",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^10.0.5"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**",
      "!**/src/index.js",
      "!**/src/registerServiceWorker.js",
      "!**/src/tests/__snapshots__/**",
      "!**/src/tests/__snapshots__/**",
      "!**/src/api/endpoints/**",
      "!**/src/api/models/Asset.ts",
      "!**/src/api/models/ConfiguredRecommendation.ts",
      "!**/src/api/models/TemplateDetails.ts",
      "!**/src/redux/store.js",
      "!**/src/redux/combined-reducers.js",
      "!**/src/redux/SharedReducer/**",
      "!**/src/redux/ApiReducer/**",
      "!**/src/containers/Notifications/**",
      "!**/src/containers/Settings/**",
      "!**/src/containers/WorkOrders/**",
      "!**/src/containers/AppLayout/**",
      "!**/src/containers/App/**",
      "!**/src/utilities/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetTransformer.js"
    }
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "useTabs": false
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.2",
    "ajv": "^6.9.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "cross-env": "^5.2.0",
    "cypress": "^5.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.3.0",
    "nan": "^2.14.1",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.12.0",
    "typescript": "^3.7.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "rimraf ./build && react-scripts start",
    "build": "react-scripts build",
    "test": "jest -u",
    "test:cypress": "cypress open",
    "cypress:ci": "cypress run --config video=false",
    "test:ci": "start-server-and-test start http://localhost:3000 cypress:ci",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|css|md)\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
